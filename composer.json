{
    "name": "somnambulist/laravel-doctrine-tenancy",
    "type": "library",
    "description": "A multi-tenancy implementation that uses Laravel and Doctrine.",
    "keywords": [
        "doctrine",
        "laravel",
        "laravel-doctrine",
        "tenancy",
        "multi-tenancy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dave Redfern",
            "email": "info@somnambulist.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Somnambulist\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Somnambulist\\Tenancy\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "eloquent/enumeration": "^6.0",
        "illuminate/routing": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "doctrine/orm": "^2.7",
        "laravel/framework": "^7.0|^8.0",
        "symfony/var-dumper": "^5.1",
        "rcrowe/twigbridge": "^0.12"
    }
}
